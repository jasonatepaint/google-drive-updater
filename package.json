{
  "name": "google-drive-transfer",
  "version": "1.0.0",
  "description": "Moves video files from a local file source to a Google Drive account",
  "main": "index.js",
  "repository": "https://github.com/jasonatepaint/google-drive-updater",
  "scripts": {
    "upload": "node index --function uploadRecent",
    "upload:1d": "npm run upload -- --days-ago 1",
    "upload:1w": "npm run upload -- --days-ago 7",
    "upload:lastRun": "npm run upload -- --use-last-run",
    "test": "mocha test/ --opts test/mocha.opts"
  },
  "author": "jason fischer",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.0",
    "cli-spinner": "^0.2.6",
    "commander": "^2.9.0",
    "googleapis": "^17.1.0",
    "log4js": "^1.1.1",
    "mime-types": "^2.1.14",
    "moment": "^2.17.1",
    "readdir": "0.0.13",
    "typescript": "^2.2.1"
  },
  "devDependencies": {
    "@types/node": "^7.0.8",
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "ts-node": "^2.1.0"
  }
}
